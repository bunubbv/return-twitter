{
    "manifest_version": 3,
    "name": "Return Twitter",
    "description": "리브랜딩 전 Twitter로 바꾸는 확장 프로그램",
    "version": "0.1.0",
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ],
    
    "author": "me@bunubbv.com",
    "action": {
        "default_popup": "default/index.html"
    },

    "content_scripts": [
      {
        "matches": ["*://twitter.com/*"],
        "js": ["scripts/content.js"]
      }
    ],
    "icons": {
      "128": "images/icon.png"
    }
  }